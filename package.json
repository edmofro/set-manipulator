{
  "name": "set-manipulator",
  "version": "0.3.0",
  "description": "Fast set operations for javascript arrays",
  "repository": "https://github.com/edmofro/set-manipulator.git",
  "author": "Edwin Monk-Fromont <edmofro@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015 --plugins transform-es2015-for-of -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "lib"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^13.2.2",
    "babel-plugin-transform-es2015-for-of": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^13.2.3"
  }
}
